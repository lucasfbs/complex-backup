{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
	  {
	"name": "client",
        "image": "lfbds/multi-client",
        "hostname": "client",
	"essential": false
	  },
	  {
        "name": "server",
	"image": "lfbds/multi-server",
	"hostname": "api",
        "essential": false
          },
	  {
	"name": "worker",
        "image": "lfbds/multi-worker",
        "essential": false
	  },
	  {
	"name": "nginx",
        "image": "lfbds/multi-nginx",
        "essential": true,
        "portMappings": [
       {
       "hostPort": 80,
       "containerPort": 80
       } 
	],
       "links": ["client","server"]
	  }
  ]
}
